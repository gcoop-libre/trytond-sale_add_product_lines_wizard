<form col="4">
    <image name="tryton-dialog-information" xexpand="0" xfill="0"/>
    <label string="Choose the product that will be added in the selected sales"
        id="choose" yalign="0.0" xalign="0.0" xexpand="1" colspan="3"/>
    <label name="selected_sales"/>
    <field name="selected_sales"/>
    <label name="ignored_sales"/>
    <field name="ignored_sales"/>
    <group id="line_description" string="Line" colspan="4">
        <label name="product"/>
        <field name="product" colspan="3"/>
        <label name="total_amount"/>
        <field name="total_amount"/>
        <label name="dues"/>
        <field name="dues"/>
        <label name="square_meter"/>
        <field name="square_meter"/>
        <label name="first_invoice_date"/>
        <field name="first_invoice_date"/>
        <field name="analytic_accounts" colspan="4"/>
        <field name="currency_digits" invisible="1"/>
        <field name="unit_digits" invisible="1"/>
    </group>
    <group id="line_prices" colspan="4">
        <label name="unit_price"/>
        <field name="unit_price"/>
        <label name="quantity"/>
        <field name="quantity"/>
    </group>
    <group id="line_description" string="Line description" colspan="4">
        <field name="line_description" colspan="4"/>
    </group>
</form>
